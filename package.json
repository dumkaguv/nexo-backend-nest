{
  "name": "backend-nest",
  "version": "1.0.0",
  "author": "dg",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "nest start",
    "dev": "nest start --watch",
    "build": "nest build && tsc-alias -p tsconfig.build.json",
    "debug": "nest start --debug --watch",
    "prod": "node dist/src/main.js",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"prisma/**/*.ts\"",
    "lint:check": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"prisma/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma && prettier --write \"prisma/**/*.ts\" && eslint \"prisma/**/*.ts\" --fix",
    "prisma:push": "prisma db push --schema=prisma/schema.prisma && prettier --write \"prisma/**/*.ts\" && eslint \"prisma/**/*.ts\" --fix",
    "prisma:push-force": "prisma db push --schema=prisma/schema.prisma --force-reset && prettier --write \"prisma/**/*.ts\" && eslint \"prisma/**/*.ts\" --fix",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:migrate-reset": "prisma migrate reset --schema=prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma",
    "prisma:seed": "prisma db seed --schema=prisma/schema.prisma",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "{test,src}/**/*.{ts,js,json}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.7",
    "@nestjs/platform-socket.io": "^11.1.7",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/websockets": "^11.1.7",
    "@prisma/client": "^6.17.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-security": "^3.0.1",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.12.1",
    "socket.io": "^4.8.1",
    "streamifier": "^0.1.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@brakebein/prisma-generator-nestjs-dto": "^1.28.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.38.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/sanitize-html": "^2.11.0",
    "@types/streamifier": "^0.1.2",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swagger-themes": "^1.4.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.46.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!uuid/)"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
